  /usr/share/apps/QtCurve/Glow.qtcurve
  /usr/share/apps/QtCurve/Human.qtcurve
  /usr/share/apps/QtCurve/Inverted.qtcurve
  /usr/share/apps/QtCurve/Murrine.qtcurve
  /usr/share/apps/QtCurve/Plain.qtcurve
  /usr/share/apps/QtCurve/Plastic.qtcurve
  /usr/share/apps/QtCurve/Shiny_Glass.qtcurve
  /usr/share/apps/QtCurve/Silk.qtcurve
  /usr/share/apps/kdisplay/color-schemes/QtCurve.kcsrc
  /usr/share/apps/kstyle/themes/qtc_klearlooks.themerc
